{
  "@openmrs/esm-patient-chart-app": {
    "extensionSlots": {
      "patient-chart-encounters-dashboard-slot": {
        "remove": [
          "past-visits-detail-overview"
        ]
      }
    }
  },
  "@openmrs/esm-service-queues-app": {
    "priorityConfigs": [
      {
        "conceptUuid": "037446f4-adfc-40b3-928c-a39a4826b1bf",
        "style": null,
        "color": "red"
      },
      {
        "conceptUuid": "80cd8f8c-5d82-4cdc-b96e-a6addeb94b7f",
        "style": null,
        "color": "green"
      },
      {
        "conceptUuid": "ef5718cd-d6d5-4fcf-bb82-cc4afaa90603",
        "style": null,
        "color": "orange"
      }
    ],
    "queueTables": {
      "columnDefinitions": [
        {
          "id": "transfer-status",
          "columnType": "extension",
          "header": "Transfer"
        }
      ],
      "tableDefinitions": [
        {
          "columns": [
            "patient-name",
            "coming-from",
            "priority",
            "status",
            "transfer-status",
            "queue",
            "wait-time",
            "actions"
          ]
        }
      ]
    }
  },
  "@openmrs/esm-patient-notes-app": {
    "disabled": true
  },
  "@kenyaemr/esm-patient-registration-app": {
    "defaultPatientIdentifierTypes": [
      "49af6cdc-7968-4abb-bf46-de10d7f4859f",
      "68449e5a-8829-44dd-bfef-c9c8cf2cb9b2",
      "6428800b-5a8c-4f77-a285-8d5f6174e5fb",
      "b4d66522-11fc-45c7-83e3-39a1af21ae0d",
      "be9beef6-aacc-4e1f-ac4e-5babeaa1e303",
      "ca125004-e8af-445d-9436-a43684150f8b",
      "52c3c0c3-05b8-4b26-930e-2a6a54e14c90",
      "24aedd37-b5be-4e08-8311-3721b8d5100d"
    ],
    "sections": [
      "demographics",
      "addressInformation",
      "otherInformation",
      "relationships",
      "death"
    ],
    "sectionDefinitions": [
      {
        "id": "addressInformation",
        "name": "Address information",
        "fields": [
          "address.countyDistrict",
          "address.stateProvince",
          "address.address4",
          "address.address2"
        ]
      },
      {
        "id": "otherInformation",
        "name": "Other Information",
        "fields": [
          "telephoneContact",
          "email",
          "contactLivingWithPatient"
        ]
      },
      {
        "id": "relationships",
        "name": "Relationships",
        "fields": [
          "nextOfKinName",
          "nextOfkinRelationsip",
          "nextOfkinPhone",
          "nextOfKinAddress"
        ]
      }
    ],
    "fieldDefinitions": [
      {
        "id": "address.countyDistrict",
        "type": "address",
        "label": "Region"
      },
      {
        "id": "address.stateProvince",
        "type": "address",
        "label": "Zone"
      },
      {
        "id": "address.address4",
        "type": "address",
        "label": "Woreda"
      },
      {
        "id": "address.address2",
        "type": "address",
        "label": "House number"
      },
      {
        "id": "telephoneContact",
        "type": "person attribute",
        "label": "Phone Number",
        "uuid": "b2c38640-2603-4629-aebd-3b54f33f1e3a"
      },
      {
        "id": "contactLivingWithPatient",
        "type": "person attribute",
        "label": "Primary Contact",
        "uuid": "35a08d84-9f80-4991-92b4-c4ae5903536e"
      },
      {
        "id": "email",
        "type": "person attribute",
        "uuid": "b995b277-c3cc-11ed-904a-70b5e843dbc9",
        "label": "Email address",
        "validation": {
          "matches": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$"
        }
      },
      {
        "id": "healthCenter",
        "type": "person attribute",
        "uuid": "27573398-4651-4ce5-89d8-abec5998165c",
        "label": "Nearest Health Center ",
        "validation": {
          "required": false
        }
      },
      {
        "id": "nextOfKinName",
        "type": "person attribute",
        "uuid": "830bef6d-b01f-449d-9f8d-ac0fede8dbd3",
        "label": "Name"
      },
      {
        "id": "nextOfkinRelationsip",
        "type": "person attribute",
        "uuid": "d0aa9fd1-2ac5-45d8-9c5e-4317c622c8f5",
        "label": "Relationship"
      },
      {
        "id": "nextOfkinPhone",
        "type": "person attribute",
        "uuid": "342a1d39-c541-4b29-8818-930916f4c2dc",
        "label": "Phone Number",
        "validation": {
          "matches": "^0([0-9](?:(?:[0-9][0-9])|(?:0[0-8])|(4[0-1]))[0-9]{6})$"
        }
      },
      {
        "id": "nextOfKinAddress",
        "type": "person attribute",
        "uuid": "7cf22bec-d90a-46ad-9f48-035952261294",
        "label": "Postal Address"
      }
    ],
    "registrationObs": {
      "encounterTypeUuid": "de1f9d67-b73e-4e1b-90d0-036166fc6995",
      "registrationFormUuid": "add7abdc-59d1-11e8-9c2d-fa7ae01bbebc"
    },
    "fieldConfigurations": {
      "name": {
        "displayCapturePhoto": false,
        "allowUnidentifiedPatients": false
      },
      "dateOfBirth": {
        "useEstimatedDateOfBirth": {
          "enabled": true,
          "dayOfMonth": 15,
          "month": 5
        }
      },
      "gender": [
        {
          "value": "male",
          "label": "Male"
        },
        {
          "value": "female",
          "label": "Female"
        }
      ],
      "address": {
        "useAddressHierarchy": {
          "useAddressHierarchyLabel": {
            "countyDistrict": "Region",
            "address4": "Woreda",
            "stateProvince": "Zone"
          },
          "searchAddressByLevel": true
        }
      },
      "phone": {
        "personAttributeUuid": "b2c38640-2603-4629-aebd-3b54f33f1e3a",
        "validation": {
          "required": false,
          "matches": "^0([0-9](?:(?:[0-9][0-9])|(?:0[0-8])|(4[0-1]))[0-9]{6})$"
        }
      },
      "causeOfDeath": {
        "conceptUuid": "1599AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
        "required": false
      }
    }
  }
}